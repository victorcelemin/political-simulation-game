<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 600">
  <defs>
    <pattern id="grid" width="40" height="40" patternUnits="userSpaceOnUse">
      <path d="M 40 0 L 0 0 0 40" fill="none" stroke="#5e2d79" stroke-width="0.5"/>
    </pattern>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="600" fill="#2b5ea6"/>
  <rect width="800" height="600" fill="url(#grid)" opacity="0.2"/>
  
  <!-- Gradient overlay -->
  <rect width="800" height="600" fill="url(#grad)" opacity="0.3"/>
  <defs>
    <linearGradient id="grad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#5e2d79;stop-opacity:0.3" />
      <stop offset="100%" style="stop-color:#e6b800;stop-opacity:0.1" />
    </linearGradient>
  </defs>
  
  <!-- Voting booths -->
  <g id="booth1">
    <rect x="100" y="200" width="120" height="200" fill="#a6231f" stroke="#e6b800" stroke-width="3"/>
    <rect x="120" y="240" width="80" height="120" fill="#e6b800"/>
    <circle cx="160" cy="280" r="20" fill="#a6231f" stroke="#222034" stroke-width="2"/>
    <circle cx="160" cy="340" r="20" fill="#a6231f" stroke="#222034" stroke-width="2"/>
  </g>
  
  <g id="booth2">
    <rect x="350" y="200" width="120" height="200" fill="#3b9c3b" stroke="#e6b800" stroke-width="3"/>
    <rect x="370" y="240" width="80" height="120" fill="#e6b800"/>
    <circle cx="410" cy="280" r="20" fill="#3b9c3b" stroke="#222034" stroke-width="2"/>
    <circle cx="410" cy="340" r="20" fill="#3b9c3b" stroke="#222034" stroke-width="2"/>
  </g>
  
  <g id="booth3">
    <rect x="600" y="200" width="120" height="200" fill="#2b5ea6" stroke="#e6b800" stroke-width="3"/>
    <rect x="620" y="240" width="80" height="120" fill="#e6b800"/>
    <circle cx="660" cy="280" r="20" fill="#2b5ea6" stroke="#222034" stroke-width="2"/>
    <circle cx="660" cy="340" r="20" fill="#2b5ea6" stroke="#222034" stroke-width="2"/>
  </g>
  
  <!-- Voting signs -->
  <text x="160" y="450" font-family="Arial" font-size="24" font-weight="bold" fill="#e6b800" text-anchor="middle">VOTA</text>
  <text x="410" y="450" font-family="Arial" font-size="24" font-weight="bold" fill="#e6b800" text-anchor="middle">ELIGE</text>
  <text x="660" y="450" font-family="Arial" font-size="24" font-weight="bold" fill="#e6b800" text-anchor="middle">DECIDE</text>
</svg>
